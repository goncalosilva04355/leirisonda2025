{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "src/App_*.tsx",
    "src/App_backup.tsx",
    "src/App_broken.tsx",
    "src/App_full.tsx",
    "src/App_simple.tsx",
    "src/App_BACKUP_STABLE.tsx"
  ]
}
